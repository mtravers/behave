(in-package :ccl);; ****************************************************************;;;; Author: Alan Ruttenberg, MIT Media Lab;; email:  alanr-d@media.mit.edu;;;; Created: Tuesday February 11,1992;; ;; ****************************************************************(in-package :ccl)(defmacro with-accessor-values (accessors instance &body body)  (let ((instance-sym (make-symbol "INSTANCE")))    `(let* ((,instance-sym ,instance)           ,@(loop for accessor in accessors collecting `(,accessor (,accessor ,instance-sym))))       ,@body)))