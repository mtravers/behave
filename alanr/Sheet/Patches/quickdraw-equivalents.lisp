(in-package :ccl)(defmethod clear-border ((b osr-bitmap))  (drawing-to-osr-bitmap    (with-white-drawing      (#_framerect (rect (view-size b))))) (invalidate-view b))(defmethod partition-by-space ((b osr-bitmap) spacing &optional offset)  (drawing-to-osr-bitmap    (with-white-drawing      (loop for xpos from offset by spacing below (point-h (view-size b))            with bottom = (point-v (view-size b))            do            (#_moveto xpos 0)            (#_lineto xpos bottom)))))        