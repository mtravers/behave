(in-package :beh)(defmethod* compile-agent ((agent vft-agent))  (let ((*agent* agent)        (*species* species))    (setf compiled-ok? t)               ; default to t    (let ((condition (parse-type 'condition (car (car body))))          (actions (mapcar #'(lambda (clause)                          (parse-type 'action clause))                               (cdr (car body)))))      (setf vft-code            (if (eq (car condition) 'not)       ; special handling for NOT              `(,(cadr condition)                NoMoreActions                ,@actions                NoMoreActions)              `(,condition                 ,@actions                NoMoreActions                NoMoreActions))))))(defmethod* download ((b agent-block))  (when species    (register-behavior (species-named species))))(defmethod start-agent-block ((b agent-block))  (download b))(defmethod stop-agent-block ((b agent-block))  (download b))(defun initialize-world ()  )